<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
						http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="WlPortalPortlet.ALL" parent="fieldListDefinition">
		<constructor-arg>
			<list>
				<ref bean="WlPortalPortlet.portletName" />
				<ref bean="WlPortalPortlet.title" />
				<ref bean="WlPortalPortlet.description" />
				<ref bean="WlPortalPortlet.screenshot" />
				<ref bean="WlPortalPortlet.type" />
				<ref bean="WlPortalPortlet.src" />
				<ref bean="WlPortalPortlet.parameters" />
				<ref bean="WlPortalPortlet.width" />
				<ref bean="WlPortalPortlet.height" />
				<ref bean="WlPortalPortlet.seq" />
				<ref bean="WlPortalPortlet.defaultDisplay" />
				<ref bean="WlPortalPortlet.status" />
				<ref bean="WlPortalPortlet.thirdpartyPortlet" />
				<ref bean="WlPortalPortlet.columnIndex" />
				<ref bean="WlPortalPortlet.icon" />
			</list>
		</constructor-arg>
	</bean>

	<bean id="WlPortalPortlet.portletId" parent="fieldDefinition">
		<property name="fieldName" value="portletId" />
		<property name="label" value="编号" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="false" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="WlPortalPortlet.portletName" parent="fieldDefinition">
		<property name="fieldName" value="portletName" />
		<property name="label" value="名称" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="256" />
		<property name="width" value="200" />
	</bean>

	<bean id="WlPortalPortlet.title" parent="fieldDefinition">
		<property name="fieldName" value="title" />
		<property name="label" value="标题" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="256" />
		<property name="width" value="200" />
	</bean>

	<bean id="WlPortalPortlet.description" parent="fieldDefinition">
		<property name="fieldName" value="description" />
		<property name="label" value="组件描述" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="512" />
		<property name="width" value="200" />
	</bean>

	<bean id="WlPortalPortlet.screenshot" parent="fieldDefinition">
		<property name="fieldName" value="screenshot" />
		<property name="label" value="截图" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="WlPortalPortlet.type" parent="fieldDefinition">
		<property name="fieldName" value="type" />
		<property name="label" value="类型" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="10" />
		<property name="width" value="100" />
	</bean>

	<bean id="WlPortalPortlet.src" parent="fieldDefinition">
		<property name="fieldName" value="src" />
		<property name="label" value="地址" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="300" />
		<property name="width" value="200" />
	</bean>

	<bean id="WlPortalPortlet.parameters" parent="fieldDefinition">
		<property name="fieldName" value="parameters" />
		<property name="label" value="参数" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="256" />
		<property name="width" value="200" />
	</bean>

	<bean id="WlPortalPortlet.width" parent="fieldDefinition">
		<property name="fieldName" value="width" />
		<property name="label" value="宽度" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="20" />
		<property name="width" value="200" />
	</bean>

	<bean id="WlPortalPortlet.height" parent="fieldDefinition">
		<property name="fieldName" value="height" />
		<property name="label" value="高度" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="20" />
		<property name="width" value="200" />
	</bean>

	<bean id="WlPortalPortlet.seq" parent="fieldDefinition">
		<property name="fieldName" value="seq" />
		<property name="label" value="顺序号" />
		<property name="fieldType" value="int" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="precision" value="5" />
		<property name="width" value="100" />
	</bean>

	<bean id="WlPortalPortlet.creator" parent="fieldDefinition">
		<property name="fieldName" value="creator" />
		<property name="label" value="creator" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="WlPortalPortlet.createTime" parent="fieldDefinition">
		<property name="fieldName" value="createTime" />
		<property name="label" value="createTime" />
		<property name="fieldType" value="datetime" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="width" value="190" />
	</bean>

	<bean id="WlPortalPortlet.modifier" parent="fieldDefinition">
		<property name="fieldName" value="modifier" />
		<property name="label" value="modifier" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="WlPortalPortlet.modifyTime" parent="fieldDefinition">
		<property name="fieldName" value="modifyTime" />
		<property name="label" value="modifyTime" />
		<property name="fieldType" value="datetime" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="width" value="190" />
	</bean>

	<bean id="WlPortalPortlet.recVer" parent="fieldDefinition">
		<property name="fieldName" value="recVer" />
		<property name="label" value="recVer" />
		<property name="fieldType" value="int" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="precision" value="10" />
		<property name="width" value="100" />
	</bean>

	<bean id="WlPortalPortlet.defaultDisplay" parent="fieldDefinition">
		<property name="fieldName" value="defaultDisplay" />
		<property name="label" value="是否默认显示" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="10" />
		<property name="width" value="100" />
	</bean>

	<bean id="WlPortalPortlet.status" parent="fieldDefinition">
		<property name="fieldName" value="status" />
		<property name="label" value="状态" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="10" />
		<property name="width" value="100" />
	</bean>

	<bean id="WlPortalPortlet.thirdpartyPortlet" parent="fieldDefinition">
		<property name="fieldName" value="thirdpartyPortlet" />
		<property name="label" value="第三方组件" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="10" />
		<property name="width" value="100" />
	</bean>
	
	<bean id="WlPortalPortlet.columnIndex" parent="fieldDefinition">
		<property name="fieldName" value="columnIndex" />
		<property name="label" value="归属列" />
		<property name="fieldType" value="int" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="precision" value="5" />
		<property name="width" value="100" />
	</bean>
	
	<bean id="WlPortalPortlet.icon" parent="fieldDefinition">
		<property name="fieldName" value="portletName" />
		<property name="label" value="小图标" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="250" />
		<property name="width" value="200" />
	</bean>

</beans>
