<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
						http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="OaUserAnnouncement.ALL" parent="fieldListDefinition">
		<constructor-arg>
			<list>
				<ref bean="OaUserAnnouncement.announcementId" />
				<ref bean="OaUserAnnouncement.receiveUserId" />
				<ref bean="OaUserAnnouncement.readTime" />
				<ref bean="OaUserAnnouncement.readFlag" />
				<ref bean="OaUserAnnouncement.feedInfo" />
				<ref bean="OaUserAnnouncement.feedOrgId" />
				<ref bean="OaUserAnnouncement.actionTarget" />
			</list>
		</constructor-arg>
	</bean>

	<bean id="OaUserAnnouncement.id" parent="fieldDefinition">
		<property name="fieldName" value="id" />
		<property name="label" value="ID" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="false" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="OaUserAnnouncement.announcementId" parent="fieldDefinition">
		<property name="fieldName" value="announcementId" />
		<property name="label" value="公告ID" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="OaUserAnnouncement.receiveUserId" parent="fieldDefinition">
		<property name="fieldName" value="receiveUserId" />
		<property name="label" value="接收人" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="OaUserAnnouncement.readTime" parent="fieldDefinition">
		<property name="fieldName" value="readTime" />
		<property name="label" value="阅读时间" />
		<property name="fieldType" value="datetime" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="width" value="190" />
	</bean>

	<bean id="OaUserAnnouncement.readFlag" parent="fieldDefinition">
		<property name="fieldName" value="readFlag" />
		<property name="label" value="阅读标记" />
		<property name="fieldType" value="selectCode.readFlag" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="width" value="200" />
	</bean>

	<bean id="OaUserAnnouncement.feedInfo" parent="fieldDefinition">
		<property name="fieldName" value="feedInfo" />
		<property name="label" value="反馈内容" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="500" />
		<property name="width" value="200" />
	</bean>

	<bean id="OaUserAnnouncement.feedOrgId" parent="fieldDefinition">
		<property name="fieldName" value="feedOrgId" />
		<property name="label" value="feedOrgId" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>
	
	<bean id="OaUserAnnouncement.feedStatus" parent="fieldDefinition">
		<property name="fieldName" value="feedStatus" />
		<property name="label" value="feedStatus" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="OaUserAnnouncement.creator" parent="fieldDefinition">
		<property name="fieldName" value="creator" />
		<property name="label" value="creator" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="OaUserAnnouncement.createTime" parent="fieldDefinition">
		<property name="fieldName" value="createTime" />
		<property name="label" value="createTime" />
		<property name="fieldType" value="datetime" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="width" value="190" />
	</bean>

	<bean id="OaUserAnnouncement.modifier" parent="fieldDefinition">
		<property name="fieldName" value="modifier" />
		<property name="label" value="modifier" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="OaUserAnnouncement.modifyTime" parent="fieldDefinition">
		<property name="fieldName" value="modifyTime" />
		<property name="label" value="modifyTime" />
		<property name="fieldType" value="datetime" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="width" value="190" />
	</bean>

	<bean id="OaUserAnnouncement.recVer" parent="fieldDefinition">
		<property name="fieldName" value="recVer" />
		<property name="label" value="recVer" />
		<property name="fieldType" value="int" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="precision" value="10" />
		<property name="width" value="100" />
	</bean>

	<bean id="OaUserAnnouncement.actionTarget" parent="fieldDefinition">
		<property name="fieldName" value="actionTarget" />
		<property name="label" value="执行目标" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="512" />
		<property name="width" value="200" />
	</bean>

</beans>
